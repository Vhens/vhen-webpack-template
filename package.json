    "image-webpack-loader": "^7.0.1",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.3",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^4.2.3",
